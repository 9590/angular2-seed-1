{
  "name": "angular2-webpack2-starter",
  "version": "0.0.0",
  "description": "An Angular 2 Starter kit featuring Angular 2 and Webpack 2",
  "author": "PatrickJS <github@gdi2290.com>",
  "main": "index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev -- --env.HMR",
    "server:dev": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --env.ENV development",
    "build": "node_modules/webpack/bin/webpack.js --env.ENV development",
    "typings-install": "node_modules/typings/dist/bin.js install",
    "postinstall": "npm run typings-install"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "~0.6.11"
  },
  "devDependencies": {
    "awesome-typescript-loader": "~v0.17.0-rc.5",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.2",
    "ts-node": "^0.6.2",
    "typescript": "~1.8.9",
    "typings": "~0.7.12",
    "webpack": "~2.1.0-beta.5",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "v2.0.0-beta"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gdi2290/angular2-webpack2-starter/issues"
  },
  "homepage": "https://github.com/gdi2290/angular2-webpack2-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gdi2290/angular2-webpack2-starter.git"
  }
}
